<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="local.DBusService">
    <property name="title" type="s" access="readwrite"/>
    <property name="lyrics" type="as" access="readwrite"/>
    <property name="translatedLyrics" type="as" access="readwrite"/>
    <property name="currentLine" type="s" access="readwrite"/>
    <property name="translatedLine" type="s" access="readwrite"/>
    <property name="index" type="n" access="readwrite"/>
    <signal name="titleChanged">
      <arg type="s" direction="out"/>
    </signal>
    <signal name="lyricsChanged">
      <arg type="as" direction="out"/>
    </signal>
    <signal name="translatedLyricsChanged">
      <arg type="as" direction="out"/>
    </signal>
    <signal name="currentLineChanged">
      <arg type="s" direction="out"/>
    </signal>
    <signal name="translatedLineChanged">
      <arg type="s" direction="out"/>
    </signal>
    <signal name="indexChanged">
      <arg type="n" direction="out"/>
    </signal>
  </interface>
</node>
